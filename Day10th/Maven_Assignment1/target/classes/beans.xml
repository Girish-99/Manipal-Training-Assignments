<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<bean id="line" class ="com.manipal.bean.Line">   <!-- Welcome welcome= new Welcome()-->
		 <constructor-arg name="p1" ref= "pointl1"></constructor-arg>
		 <constructor-arg name="p2" ref= "pointl2"></constructor-arg>
	</bean>
	
	<bean id="triangle" class ="com.manipal.bean.Triangle">   <!-- Welcome welcome= new Welcome()-->
		 <constructor-arg name="t1" ref= "pointt1"></constructor-arg>
		 <constructor-arg name="t2" ref= "pointt2"></constructor-arg>
		 <constructor-arg name="t3" ref= "pointt3"></constructor-arg>
	</bean>
	
	<bean id="pointl1" class ="com.manipal.bean.Point">   <!-- Welcome welcome= new Welcome()-->
		 <constructor-arg name="x" value="3"></constructor-arg>
		 <constructor-arg name="y" value="4"></constructor-arg>
	</bean>
	
	<bean id="pointl2" class ="com.manipal.bean.Point">   <!-- Welcome welcome= new Welcome()-->
		 <constructor-arg name="x" value="5"></constructor-arg>
		 <constructor-arg name="y" value="6"></constructor-arg>
	</bean>
	
	<bean id="pointt1" class ="com.manipal.bean.Point">   <!-- Welcome welcome= new Welcome()-->
		 <constructor-arg name="x" value="7"></constructor-arg>
		 <constructor-arg name="y" value="8"></constructor-arg>
	</bean>
	
	<bean id="pointt2" class ="com.manipal.bean.Point">   <!-- Welcome welcome= new Welcome()-->
		 <constructor-arg name="x" value="9"></constructor-arg>
		 <constructor-arg name="y" value="10"></constructor-arg>
	</bean>
	
	<bean id="pointt3" class ="com.manipal.bean.Point">   <!-- Welcome welcome= new Welcome()-->
		 <constructor-arg name="x" value="11"></constructor-arg>
		 <constructor-arg name="y" value="12"></constructor-arg>
	</bean>
	
</beans>